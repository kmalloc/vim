#!/bin/sh

rm -f ~/.vim/caches/filenametags
echo -e "!_TAG_FILE_SORTED\t2\t/2=foldcase" > ~/.vim/caches/filenametags
find ~/code/gui_tflex/  -regex '.*\.\(h\|c\|cpp\|cc\|cxx\|py\|mk\|xrc\|txt\|Makefile\)' -type f -printf "%f\t%p\t1\n" | \
sort -f >> ~/.vim/caches/filenametags


